  "extraction_patterns": {
    "company_patterns": [
      "\\b([A-Z][A-Za-z]+(?:\\s+[A-Z][A-Za-z]+){0,3})\\s+(?:Pvt\\.?\\s*Ltd\\.?|Private\\s+Limited|Limited|Ltd\\.?|Inc\\.?|Incorporated|Corp\\.?|Corporation|LLC|Technologies|Systems|Solutions|Software|Consulting|Services|Group|Enterprises|Industries)\\b",
      "\\b([A-Z][A-Za-z]{2,}(?:\\s+[A-Z][A-Za-z]+)?)\\s+(?:Tech|IT|Labs|Studio|Agency)\\b"
    ],
    
    "salary_patterns": [
      "(\\d+(?:\\.\\d+)?)\\s*(?:lpa|lakhs?\\s+per\\s+annum|l\\.?p\\.?a\\.?)",
      "(\\d+)\\s*(?:k|thousand)\\s*(?:per\\s+month|pm|/month)",
      "(?:ctc|package|salary)\\s*:?\\s*(?:rs\\.?\\s*)?(\\d+(?:\\.\\d+)?)\\s*(?:lpa|lakhs?)",
      "(?:stipend|compensation)\\s*:?\\s*(?:rs\\.?\\s*)?(\\d+(?:,\\d+)?)\\s*(?:per\\s+month)?"
    ],
    
    "experience_patterns": [
      "(\\d+)\\s*(?:\\+|to|-)\\s*(\\d+)\\s*years?\\s+(?:of\\s+)?experience",
      "(\\d+)\\s*years?\\s+(?:of\\s+)?experience",
      "\\b(fresher|freshers?)\\b",
      "\\b(entry\\s+level)\\b",
      "\\b0\\s*(?:\\+|to|-)\\s*(\\d+)\\s*years?\\b"
    ]
  },
  
  "text_cleaning": {
    "remove_patterns": [
      "(?:best\\s+)?regards?,?.*",
      "thanks?\\s+(?:and\\s+)?regards?.*",
      "sincerely,?.*",
      "warm\\s+regards?.*",
      "------+\\s*forwarded\\s+message\\s*------+.*",
      "from:.*?(?:sent:|to:|subject:).*",
      "----+\\s*original\\s+message\\s*----+.*",
      "on\\s+\\w+,\\s+\\w+\\s+\\d+,\\s+\\d+.*?wrote:.*",
      "(?:fwd?|fw|re):.*?\\n",
      "this\\s+(?:e-?mail|message).*?confidential.*",
      "disclaimer:.*",
      "confidentiality\\s+notice:.*",
      "(?:please\\s+)?do\\s+not\\s+reply.*",
      "this\\s+is\\s+an?\\s+automated.*",
      "unsubscribe.*",
      "sent\\s+from\\s+my.*",
      "get\\s+outlook\\s+for.*"
    ],
    "min_line_length": 5
  },
  
  "processing": {
    "batch_size": 20,
    "text_limit": 5000
  }
}
